#include<stdio.h>
int square(int num,int pow);
int main(){
int num,pow;
printf("enter the number\n");
scanf("%d",&num);
printf("enter the power\n");
scanf("%d",&pow);
square(num,pow);
return 0;
}
int square(int num,int pow){
    int i,sq=1;
    for(i=0;i<pow;i++){
        sq=sq*num;
    }
    printf("The square of %d is %d",num,sq);
    return 0;
}
